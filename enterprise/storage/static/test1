		<script src="http://pic.vronline.com/website/js/three.min.js?5"></script>
		<script src="http://pic.vronline.com/website/js/jquery.valiant360.js?5"></script>
		<script src="http://pic.vronline.com/website/js/bannerVideo.js?5"></script>
		<script src="http://pic.vronline.com/common/js/platform.js?5"></script>
		<script src="http://pic.vronline.com/website/js/base.js?5"></script>
		<script type="text/javascript">
		//导航条
		$('.nav ul li').each(function(){
			$(this).mouseover(function(){
				$(this).children('.con').show();
			});
			$(this).mouseout(function(){
				$(this).children('.con').hide();
			})
		});

		$(".website-link").click(function(){
			var link_to=$(this).attr("link-to");
			if(!link_to){
				return false;
			}
			window.location.href=link_to;
		});

		/**
		 * VR游戏详情
		 */
		$("body").delegate(".show-vrgame-detail", 'click', function(event) {
			var appid=$(this).attr("appid");
			var json={
				game_id:appid
			};
			PL.callFun('gamelistframe', 'item_clicked', json);
			location.href = "/vrgame/" + appid;
		});

		/**
		 * 页游详情
		 */
		$("body").delegate('.show-webgame-detail', 'click', function(event) {
	    	var appid=$(this).attr("appid");
	    	console.log(appid);
	    	window.location.href="/webgame/"+appid;
	    });

		/**
		 * 开始页游，进入选服务器
		 */
	    $("body").delegate('.start-web-game-server', 'click', function(event) {
	    	var appid=$(this).attr("game-id");
	    	var appname=$(this).attr("game-name");
	    	var json={
	    		gameid:appid,
	        	gamename:appname,
	        	gameSrc:ClientConfig.Host + "/servers/" + appid
	    	};
	    	PL.startGame(json);
	    });

	    /**
	     * 页游进入具体服务器
	     */
	    $("body").delegate('.start-web-game', 'click', function(event) {
	    	var gameid=$(this).attr("game-id");
			if(!gameid){
				return false;
			}
			var json={
				gameid:gameid,
				gamename: $(this).attr("game-name"),
				areaid:$(this).attr("server-id"),
				areaname:$(this).attr("server-name")
			};
			PL.startGame(json);
	    });

		</script>
